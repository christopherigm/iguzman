{
  "name": "@repo/i18n",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "check-types": "tsc --noEmit"
  },
  "exports": {
    "./config": {
      "types": "./src/config.ts",
      "default": "./src/config.ts"
    },
    "./routing": {
      "types": "./src/routing.ts",
      "default": "./src/routing.ts"
    },
    "./navigation": {
      "types": "./src/navigation.ts",
      "default": "./src/navigation.ts"
    },
    "./request": {
      "types": "./src/request.ts",
      "default": "./src/request.ts"
    },
    "./messages/en": "./messages/en.json",
    "./messages/es": "./messages/es.json"
  },
  "dependencies": {
    "next-intl": "^4"
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "typescript": "5.9.2"
  }
}
